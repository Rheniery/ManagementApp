
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Member</h1>

@if (ViewData["ValidMember"] != null)
{
    @if (ViewData["ValidMember"].ToString() == "Error")
    {
<div class="alert alert-danger alert-dismissible fade show" role="alert">
    <strong>Failed!</strong> It was not possible to save the register.
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
    }
    @if (ViewData["ValidMember"].ToString() == "Success")
    {
        <div class="alert alert-success alert-dismissible fade show" role="alert">
            <strong>Success!!</strong> The member was saved.
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    }
}